{
  "name": "@nutgaard/gitnext",
  "version": "0.0.1-beta.14",
  "description": "Small cli to help prioritize your github tasks",
  "main": "./lib/index.js",
  "bin": {
    "gitnext": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutgaard/gitnext"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "server": "npm run build && node lib/server/index.js",
    "create": "npm run build && npm run test",
    "build": "npm run build:clean && npm run build:tsc && npm run build:copy-resources",
    "build:clean": "rm -rf lib",
    "build:tsc": "tsc -p .",
    "build:copy-resources": "cp -r src/graphql-queries lib && cp src/default-config.yaml lib",
    "test": "jest",
    "test:types":  "babel src/typecheck --out-dir types-detailed --extensions \".ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "media"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "daemonize2": "^0.4.2",
    "detect-port": "^1.3.0",
    "emoji-strip": "^1.0.1",
    "express": "^4.17.1",
    "fastest-levenshtein": "^1.0.12",
    "ink": "^3.0.8",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-select-input": "^4.2.0",
    "ink-spinner": "^4.0.1",
    "javascript-time-ago": "^2.3.4",
    "js-yaml": "^4.0.0",
    "meow": "^9.0.0",
    "node-fetch": "^2.6.1",
    "open": "8.0.2",
    "react": "^17.0.1",
    "typecheck.macro": "^0.7.2",
    "typescript-json-decoder": "^1.0.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/cli": "^7.13.14",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@types/chalk": "^2.2.0",
    "@types/detect-port": "^1.3.0",
    "@types/emoji-strip": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/ink": "^2.0.3",
    "@types/ink-big-text": "^1.2.0",
    "@types/ink-gradient": "^2.0.1",
    "@types/javascript-time-ago": "^2.0.2",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.3",
    "@types/ws": "^7.4.0",
    "babel-plugin-macros": "^3.0.1",
    "jest": "^26.6.3",
    "typescript": "^4.2.3"
  },
  "jest": {
    "verbose": true
  },
  "author": "Nicklas Utgaard",
  "license": "MIT"
}
